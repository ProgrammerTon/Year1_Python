def Input():
 score = int(input("Enter your score : "))
 return score

def InputAgain():
 score = int(input("Error score... Enter your score again : "))
 return score

def Check(score):
 if score>=80: print("Grade A")
 elif score>=70: print("Grade B")
 elif score>=60: print("Grade C")
 elif score>=50: print("Grade D")
 else: print("Grade F")
A = Input()
if A<0 or A>100:
 a=1
 while a>0:
  B = InputAgain()
  if B>=0 and B<=100:
   break
 Check(B)
else:
 Check(A)
